class Person
{
	
	private String name;
	
	protected int age;

	public char sex;

	int score;
	
}



class Student extends Person
{
	
	public void showInfo()
	{
		int age = this.age;
		
		int score = this.score;
		
		int sex = this.sex;
			
		System.out.println(age+"  "+score+"  "+score);
		
	}

}


class Teacher extends Student
{
	
	public void showInfo()
	{
		int age = this.age;
		
		int score = this.score;
		
		int sex = this.sex;
			
		System.out.println(age+"  "+score+"  "+score);
		
	}
		
}


class Proffesion extends Teacher
{

	public void showInfo()
	{
		int age = this.age;
		
		int score = this.score;
		
		int sex = this.sex;
			
		System.out.println(age+"  "+score+"  "+score);	
	}

}



